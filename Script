--loading noti
game.StarterGui:SetCore("SendNotification",{
    Title = "Fear dahood";
    Text = "Fear dahood loading...";
    Duration = 3;
})

--varibles
local character = game.Players.LocalPlayer.Character.HumanoidRootPart
local player = game.Players.LocalPlayer
local humanoid = character:FindFirstChild("Humanoid")
local Chatted = game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent
local Name = game.Players.LocalPlayer.Name

--ez gun cmds
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."db" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1059.21704, 104.47982, -270.870758, 0.047084894, -6.70244447e-08, -0.998890877, -1.36415563e-08, 1, -6.77418868e-08, 0.998890877, 1.68160454e-08, 0.047084894)
            end
        end
    end
end)
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."rev" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-639.654236, 21.7499943, -128.53157, -0.87585324, 8.82249562e-09, 0.482577562, 6.85837964e-09, 1, -5.83442272e-09, -0.482577562, -1.80039805e-09, -0.87585324)
            end
        end
    end
end)
--bank cmd
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."bank" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-378.103027, 21.2499924, -345.005646, -0.999518454, 6.61867283e-10, 0.0310307723, 5.68438574e-10, 1, -3.01966252e-09, -0.0310307723, -3.00056913e-09, -0.999518454)
            end
        end
    end
end)
--drop cmd
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."drop" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                getgenv().drop = true
                while getgenv().drop == true do
                  game:GetService("ReplicatedStorage").MainEvent:FireServer('DropMoney', 10000)
                wait()
                end
            end
        end
    end
end)

Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."stop" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                getgenv().drop = false
            end
        end
    end
end)

Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."god" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
                game.Workspace.Players[Name].BodyEffects.Attacking:Destroy()
            end
        end
    end
end)

--cash aura
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."cashaura" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
				getgenv().cashaura = true
				while getgenv().cashaura == true do
					wait()
					for i, money in ipairs(game.Workspace.Ignored.Drop:GetChildren()) do
						if money.Name == "MoneyDrop" and (money.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 100 then
							fireclickdetector(money.ClickDetector)
						end
					end
				end
            end
        end
    end
end)
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."cashaura off" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
				getgenv().cashaura = false
            end
        end
    end
end)
--freeze cmd
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."freeze" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
				game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            end
        end
    end
end)
Chatted:Connect(function(Message)
    if (game:GetService("Players")[Message.FromSpeaker].UserId) == getgenv().Settings.Controller then
        if Message.Message:lower() == getgenv().Settings.Prefix.."unfreeze" then
            if game.Players.LocalPlayer.UserId == getgenv().Settings.Controller then
				game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            end
        end
    end
end)

loadstring(game:HttpGet('https://raw.githubusercontent.com/SpaceYes/Lua/Main/DaHood.Lua'))()

game.StarterGui:SetCore("SendNotification",{
	Title = "Fear dahood";
	Text = "Fear dahood loaded <3";
	Duration = 3;
})
